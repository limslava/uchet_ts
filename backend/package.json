{
  "name": "uchet_ts-backend",
  "version": "1.0.0",
  "description": "Backend for Uchet_TS vehicle accounting system",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev --name init",
    "db:studio": "npx prisma studio",
    "seed": "node src/seed.js"
  },
  "keywords": [
    "vehicle",
    "accounting",
    "inspection"
  ],
  "author": "limslava",
  "license": "ISC",
  "prisma": {
    "seed": "node src/seed.js"
  },
  "dependencies": {
    "@nestjs/platform-express": "^11.1.6",
    "@prisma/client": "^6.15.0",
    "@types/multer": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "docx": "^9.5.1",
    "dotenv": "^16.4.1",
    "esm": "^3.2.25",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.4",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
