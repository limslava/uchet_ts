    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "prisma": "^6.15.0"
  }
}
